name = "cfworker-urls"
main = "src/index.ts"
compatibility_date = "2025-01-19"
compatibility_flags = [ "nodejs_compat" ]
# Account ID / 账号ID
account_id = "******************************"

[site]
bucket = "./public"

[vars]
# Domain URL / 网站完整域名路径
FULL_URL= "https://**********/"
EDIT_SUB=false # 启用自定义路径
EDIT_LEN=8 # 自定义后缀最短长度

[[kv_namespaces]]
binding = "DATABASE"
# Cloudflare KV ID / CF KV数据库ID
id = "*****************************"

#[observability.logs]
#enabled = true